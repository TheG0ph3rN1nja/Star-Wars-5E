  <element name="Force Ability(Spellcasting)" type="Class Feature" source="GM Binder SW5E" id="ID_GMBINDER_SW5E_CLASS_FEATURE_CONSULAR_SPELLCASTING_CONSULAR">
		<description>
			<p>Charisma is your force ability for your force disciplines. You use your Charisma modifier when setting the saving throw DC for a force discipline or when making an attack roll with one.</p>
			<center>
				<p><strong>Discipline save DC</strong> = 8 + your proficiency bonus + your Charisma modifier</p>
				<p><strong>Discipline attack modifier</strong> = your proficiency bonus + your Charisma modifier</p>
			</center>
		</description>
		<sheet>
			<description>Ability to use force disciplines(spells).</description>
		</sheet>
		<spellcasting name="Consular" ability="Charisma" prepare="false" allowReplace="true">
			<list />
		</spellcasting>
		<rules>
			<grant type="Grants" id="ID_INTERNAL_GRANT_MULTICLASS_SPELLCASTING_SLOTS_FULL" requirements="ID_INTERNAL_GRANT_MULTICLASS" />
